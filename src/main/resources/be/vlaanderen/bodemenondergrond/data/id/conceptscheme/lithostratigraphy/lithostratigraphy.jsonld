                "concept:boeretang-member",
                "concept:putte-member",
                "concept:terhagen-member",
                "concept:eigenbilzen-formation"
            ],
            "prefLabel": "Rupel Group",
            "semanticRelation": [
                "concept:paleogene-neogene",
                "concept:belsele-waas-member",
                "concept:boom-formation",
                "concept:berg-member",
                "concept:bilzen-formation",
                "concept:kerniel-member",
